macos.single.debug = "./macos/godot_rtmidi.macos.debug.universal.dylib"
macos.double.debug = "./macos/godot_rtmidi.macos.debug.double.universal.dylib"
macos.single.release = "./macos/godot_rtmidi.macos.release.universal.dylib"
